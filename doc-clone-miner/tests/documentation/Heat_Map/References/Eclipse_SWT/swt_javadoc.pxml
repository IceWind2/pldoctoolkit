g = srcGreens[(data &amp; srcGreenMask) &gt;&gt;&gt; srcGreenShift] &amp; 0xff;
b = srcBlues[(data &amp; srcBlueMask) &gt;&gt;&gt; srcBlueShift] &amp; 0xff;

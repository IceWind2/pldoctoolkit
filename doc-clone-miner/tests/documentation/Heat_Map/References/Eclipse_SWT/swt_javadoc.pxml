apr = alphaY * alphaStride + alphaX;
break;
case ALPHA_MASK_PACKED:
if (alphaData == null) alphaMode = 0x10000;
alphaStride &lt;&lt;= 3;
apr = alphaY * alphaStride + alphaX;
break;
case ALPHA_MASK_INDEX:
//throw new IllegalArgumentException("Invalid alpha type");
return;
case ALPHA_MASK_RGB:
if (alphaData == null) alphaMode = 0x10000;
apr = 0;
break;
